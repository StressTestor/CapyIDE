{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:update": "playwright test --update-snapshots",
    "security:headers:test": "node scripts/headers-test.mjs",
    "security:assets:scan": "node scripts/assets-scan.mjs",
    "audit": "pnpm audit --audit-level=high"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.4.6"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "@types/node": "20.19.9",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@tailwindcss/postcss": "4.1.11",
    "tailwindcss": "4.1.11",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.6",
    "@eslint/eslintrc": "3.3.1",
    "@playwright/test": "1.49.1",
    "husky": "9.1.7",
    "prettier": "3.3.3"
  },
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@10.14.0"
}
